{
    "name": "Calculator",
    "version": "1.0.0",
    "description": "A simple Calculator extension where you can use to calculate things, without having to go a another tab or calculator",
    "manifest_version": 3,
    "action": {
        "default_icon": "/icons/icon128.png",
        "icons": {
            "16": "/icons/icon16.png",
            "48": "/icons/icon48.png",
            "96": "/icons/icon96.png",
            "128": "/icons/icon128.png"
        },
        "default_popup": "index.html",
        "default_title": "Calculator"
    },
    "author": "Bharath Kumar"
}